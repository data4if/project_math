   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "xvalues=np.linspace(-10,10,1000)\n",
    "yvalues=[]\n",
    "\n",
    "def step(x):\n",
    "    if x>=0:\n",
    "        return 1\n",
    "    else:\n",
    "        return 0\n",
    "\n",
    "for x in xvalues:\n",
    "    yvalues.append(step(x))\n",
    "\n",
    "plt.plot(yvalues,xvalues)\n",
    "plt.show()\n"
   ]
  }
